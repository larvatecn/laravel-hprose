{
    "name": "larva/laravel-hprose",
    "description": "This is a Laravel expansion for the hprose.",
    "keywords": [
        "Larva",
        "laravel",
        "hprose"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "illuminate/bus": "^^6.0 || ^7.0 || ^8.0",
        "illuminate/queue": "^6.0 || ^7.0 || ^8.0",
        "hprose/hprose": "^2.0",
        "hprose/hprose-swoole": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Larva\\Hprose\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Larva\\Hprose\\HproseServiceProvider"
            ],
            "aliases": {
                "Hprose": "Larva\\Hprose\\Hprose"
            }
        }
    },
    "prefer-stable": true
}
